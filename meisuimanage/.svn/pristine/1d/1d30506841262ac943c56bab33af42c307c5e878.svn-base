package com.meisui.manage.entity;

public class Anchor_Pay_Rule {
	private int id;
	private int type;
	private int min_virtual_count;
	private int max_virtual_count;
	private int effect_days;
	private int week_reward;
	private double divide;
	private int is_day;
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getType() {
		return type;
	}
	public void setType(int type) {
		this.type = type;
	}
	public int getMin_virtual_count() {
		return min_virtual_count;
	}
	public void setMin_virtual_count(int min_virtual_count) {
		this.min_virtual_count = min_virtual_count;
	}
	public int getMax_virtual_count() {
		return max_virtual_count;
	}
	public void setMax_virtual_count(int max_virtual_count) {
		this.max_virtual_count = max_virtual_count;
	}
	public int getEffect_days() {
		return effect_days;
	}
	public void setEffect_days(int effect_days) {
		this.effect_days = effect_days;
	}
	public int getWeek_reward() {
		return week_reward;
	}
	public void setWeek_reward(int week_reward) {
		this.week_reward = week_reward;
	}
	public double getDivide() {
		return divide;
	}
	public void setDivide(double divide) {
		this.divide = divide;
	}
	public int getIs_day() {
		return is_day;
	}
	public void setIs_day(int is_day) {
		this.is_day = is_day;
	}
}
