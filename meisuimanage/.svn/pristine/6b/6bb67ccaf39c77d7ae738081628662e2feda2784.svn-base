public class Change {
	private int id;//充值套餐id
	private int virtual_count;//虚拟币值
	private int change_rmb;//需人民币
	private String w_name;//编辑人员
	private Date create_time;//创建时间
	private int is_online;//是否上线 1是 0否
	private String update_time;//更新时间
	private int is_virtual;//是否会员价（3%）：0否1是
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public int getVirtual_count() {
		return virtual_count;
	}
	public void setVirtual_count(int virtual_count) {
		this.virtual_count = virtual_count;
	}
	public int getChange_rmb() {
		return change_rmb;
	}
	public void setChange_rmb(int change_rmb) {
		this.change_rmb = change_rmb;
	}
	public String getW_name() {
		return w_name;
	}
	public void setW_name(String w_name) {
		this.w_name = w_name;
	}
	public Date getCreate_time() {
		return create_time;
	}
	public void setCreate_time(Date create_time) {
		this.create_time = create_time;
	}
	public int getIs_online() {
		return is_online;
	}
	public void setIs_online(int is_online) {
		this.is_online = is_online;
	}
	public String getUpdate_time() {
		return update_time;
	}
	public void setUpdate_time(Date update_time) {
		this.update_time = AuthUtil.formatDateToString(update_time);
	}
	public int getIs_virtual() {
		return is_virtual;
	}
	public void setIs_virtual(int is_virtual) {
		this.is_virtual = is_virtual;
	}
	
}
